    "def remove_vowels(sentence):\n",
    "    return \"\".join([l for l in sentence if str.lower(l) not in \"aeiou\"])\n",
    "\n",
    "sentence = \"The quick brown fox jumps over the lazy dog\"\n",
    "print(\"Filtered result: \" + remove_vowels(sentence))"
   ]
